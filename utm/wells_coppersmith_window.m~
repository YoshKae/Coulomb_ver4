%-------------------------------------------------------------------------
%     Reflect results button (pushbutton)
%-------------------------------------------------------------------------
function pushbutton_reflect_Callback(hObject, eventdata, handles)
x = str2num(get(findobj('Tag','edit_wc_length'),'String'));
y = str2num(get(findobj('Tag','edit_wc_length'),'String'));

%-------------------------------------------------------------------------
%     close button (pushbutton)
%-------------------------------------------------------------------------
function pushbutton_wc_close_Callback(hObject, eventdata, handles)
    delete(figure(gcf));
    
%-------------------------------------------------------------------------
%     WC equation
%-------------------------------------------------------------------------   
function wcout = wells_coppersmith(a,b,m)
x = (m - a) / b;
wcout = 10.0.^x;


        

