% hObject    handle to figure
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)

% Get default command line output from handles structure
varargout{1} = handles.output;


%-------------------------------------------------------------------------
%     Fault sense (popupmenu)
%-------------------------------------------------------------------------
function popupmenu_fault_sense_Callback(hObject, eventdata, handles)
global IFAULT_SENSE
IFAULT_SENSE = single(get(hObject,'Value'));

% --- Executes during object creation, after setting all properties.
function popupmenu_fault_sense_CreateFcn(hObject, eventdata, handles)
global IFAULT_SENSE
IFAULT_SENSE = 1.0;
if ispc && isequal(get(hObject,'BackgroundColor'), get(0,'defaultUicontrolBackgroundColor'))
    set(hObject,'BackgroundColor','white');
end


%-------------------------------------------------------------------------
%     Mw (textfield)
%-------------------------------------------------------------------------
function edit_wc_mc_Callback(hObject, eventdata, handles)
x = str2num(get(hObject,'String'));
set(hObject,'String',num2str(x,'%4.2f'));

% --- Executes during object creation, after setting all properties.
function edit_wc_mc_CreateFcn(hObject, eventdata, handles)
if ispc && isequal(get(hObject,'BackgroundColor'), get(0,'defaultUicontrolBackgroundColor'))
    set(hObject,'BackgroundColor','white');
end
x = str2num(get(findobj('Tag','edit_eq_mo'),'String'));
set(hObject,'String',num2str(x,'%4.2f'));

%-------------------------------------------------------------------------
%     Calculation button (pushbutton)
%-------------------------------------------------------------------------
function pushbutton_wc_calc_Callback(hObject, eventdata, handles)
global IFAULT_SENSE
mw = str2num(get(findobj('Tag','edit_wc_mc'),'String'));
% ifault = get(findobj('Tag','popumenu_fault_sense'),'Value')
ifault = IFAULT_SENSE; % remove global IFAULT_SENSE for near future
if ifault == 1
    al = 4.38; bl = 1.49; aw = 4.06; bw = 2.25;   
elseif ifault == 2  % strike-slip
    al = 4.33; bl = 1.49; aw = 3.80; bw = 2.59;   
elseif ifault == 3  % reverse
    al = 4.49; bl = 1.49; aw = 4.37; bw = 1.95;     
else                % normal
    al = 4.34; bl = 1.54; aw = 4.04; bw = 2.11;    
end
    fault_l = wells_coppersmith(al,bl,mw);
    fault_w = wells_coppersmith(aw,bw,mw); 
    set(findobj('Tag','edit_wc_length'),'String',num2str(fault_l,'%6.2f'));
	set(findobj('Tag','edit_wc_width'),'String',num2str(fault_w,'%5.2f'));

%-------------------------------------------------------------------------
%     Calculated fault length (textfield)
%-------------------------------------------------------------------------
function edit_wc_length_Callback(hObject, eventdata, handles)
x = single(get(hObject,'Value'));
% 1: strike-slip
% 2: reverse
% 3: normal

% --- Executes during object creation, after setting all properties.
function edit_wc_length_CreateFcn(hObject, eventdata, handles)
% hObject    handle to edit_wc_length (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    empty - handles not created until after all CreateFcns called

% Hint: edit controls usually have a white background on Windows.
%       See ISPC and COMPUTER.
if ispc && isequal(get(hObject,'BackgroundColor'), get(0,'defaultUicontrolBackgroundColor'))
    set(hObject,'BackgroundColor','white');
end


